  "vhosts": [
    { "name": "/" }
  ],
  "queues": [
    {
      "name": "queue.temperature",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": { "x-message-ttl": 86400000 }
    },
    {
      "name": "queue.humidity",
