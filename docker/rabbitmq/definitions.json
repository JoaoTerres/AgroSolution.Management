      "auto_delete": false,
      "arguments": {}
    }
  ],
  "exchanges": [
    {
      "name": "iot.events",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "iot.dead-letter",
      "vhost": "/",
      "type": "fanout",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "iot.events",
      "vhost": "/",
      "destination": "queue.temperature",
      "destination_type": "queue",
      "routing_key": "iot.temperature",
      "arguments": {}
    },
    {
      "source": "iot.events",
      "vhost": "/",
